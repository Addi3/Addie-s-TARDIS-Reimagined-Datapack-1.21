give @p written_book[custom_model_data=1,written_book_content={title:"TARDIS Manual",author:"Gallifreyan Council",generation:0,pages:['[{"text":"Tardis Type 75 Manual","color":"gold","bold":true,"hoverEvent":{"action":"show_text","value":[{"text":"","color":"gold","bold":true}]}},{"text":"\\n\\nContent:\\n\\n","color":"black","bold":true,"hoverEvent":{"action":"show_text","value":[{"text":"","color":"gold","bold":true}]}},{"text":"-Getting the TARDIS\\n\\n","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"Page 2"},{"text":""}]}},{"text":"-Repairing the TARDIS","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"Page 3-4"}]}},{"text":"\\n\\n-Rooms","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"Page 5"}]}},{"text":"\\n\\n-Admin","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"Page 6-9"}]}},{"text":"\\n\\n-Controls","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"Page 10-16"}]}}]','[{"text":"Getting The TARDIS\\n","bold":true,"underlined":true},{"text":"\\nLocate a crashed ship underground near by at \'y=17\'. Once you\'ve located the ship you will find a tardis inside the wreckage. Make sure to bring a nether star and a tardis key! (to unlock the tardis \'by default right\' click the door)\\n\\n","bold":false,"underlined":false}]','[{"text":"Repairing the TARDIS","bold":true,"underlined":true},{"text":"\\n\\nFirstly, craft a TARDIS Key and enter the tardis and head below the console. You will come across the lower console, chuck the nether star at the lower console.","color":"black","bold":false,"underlined":false}]','{"text":"You will then receive instructions to leave the tardis, it wont repair until you have left!","color":"black","bold":false,"underlined":false}','[{"text":"Rooms","bold":true,"underlined":true},{"text":"\\n\\n-Corridors:","bold":true},{"text":"\\n\\n*Lifts\\n*Upper Levels\\n*The Depths","bold":false,"underlined":false},{"text":"\\n\\n-Rooms:","bold":true,"underlined":false},{"text":"\\n\\n*Library\\n*Dining Room\\n*Kitchen\\n*Eye of Harmoney","bold":false,"underlined":false}]','[{"text":"Admin","bold":true,"underlined":true},{"text":"\\n\\n*/function addie:admin/become_pilot to be able to use the tardis console.\\n\\n\\n*/function addie:admin/dont_play_music to not play intro music(do this when first finding the tardis and after repairing it)","bold":false,"underlined":false}]','{"text":"*/function addie:admin/get_key to get the tardis key\\n\\n*/function addie:admin/lock_all to lock all upgrades\\n\\n*/function addie:admin/unlock_all to unlock all upgrades","bold":false,"underlined":false}','{"text":"*/function addie:admin/play_music_again will play the music once more when you enter\\n\\n*/function addie:admin/play_music_again will remove your pilot role\\n"}','{"text":"*/function addie:admin/repair_tardis repairs the tardis\\n\\n*/function addie:admin/spawn_tardis spawns the tardis(dont use if you already own a tardis)\\n\\n* /function addie:admin/get_manual gives you the manual\\n"}','[{"text":"Controls","color":"black","bold":true,"underlined":true},{"text":"\\n\\n\\n-Interior Colour Buttons:","bold":true,"underlined":false},{"text":"\\nThese buttons can switch the interior light colour (technically the glass colour but it\'s visible with shaders that allow coloured lighting.","bold":false,"underlined":false}]','[{"text":"-Power Lever:","bold":true,"underlined":false},{"text":"\\nThe power lever turns off the tardis, rendering it basically useless as nothing will work (except a few controls).","bold":false,"underlined":false},{"text":"\\n\\n\\n-Monitor:","bold":true},{"text":"\\nThe monitor can be used to see the; fuel level,","bold":false}]','{"text":"current tardis position (clicking the part of the monitor where this is will change it to show the set coordinates), set / see current dimension, tardis Security - Exterior view, Shields (upgradable), HADS."}','[{"text":"-Disk Drive:","bold":true},{"text":"\\nThe disk drive is used to insert upgrades into the Tardis\'s matrix to improve its performance.\\n\\n","bold":false},{"text":"-Handbrake:","bold":true},{"text":"\\nThe handbrake is used to \'park\' the tardis, meaning the throttle won\'t work and the tardis will refuse to take off.","bold":false}]','[{"text":"-Coordinates:","bold":true},{"text":"To change X, Y, and Z click the buttons on the left (they are color coded the same as the monitors coords), on the right side are the increase buttons which are (from top to bottom) 1, 10, 100,","bold":false}]','{"text":"finally the green button confirms the coordinates and the purple one confirms the set dimension (press when in vortex flight (indicated by the sfx), in this order: set dimension button the set coordinates button. if not pressed in this order you will end up at 0 200 0 in that dimension.)","bold":false}','[{"text":"-Throttle:","bold":true},{"text":"\\nThe throttle is used to take off and land the tardis. (After taking off wait for the tardis to be in the vortex - indicated by the sfx - before landing).","bold":false}]','[{"text":"CREDITS:","color":"gold","bold":true,"underlined":false,"hoverEvent":{"action":"show_text","value":[{"text":"","color":"gold","bold":true}]}},{"text":"\\n\\nMain Dev:","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"","color":"gold","bold":true}]}},{"text":" Addie","color":"black","bold":true,"hoverEvent":{"action":"show_text","value":[{"text":"Click me","color":"gold","bold":true}]},"clickEvent":{"action":"open_url","value":"https://www.youtube.com/channel/UCvhwMmPtf0YfL07q_w_uCoA"}},{"text":"\\n\\nContributor:","color":"black","bold":false,"hoverEvent":{"action":"show_text","value":[{"text":"","color":"gold","bold":true}]}},{"text":" Make Tendo","color":"black","bold":true,"hoverEvent":{"action":"show_text","value":[{"text":"Click Me","color":"gold","bold":true}]},"clickEvent":{"